	<section class="rows padchildren">
		<div class="center em15 top">
			<%= post.created_at %>
		</div>
		<div class="left em15">
			<%= link_to post.title, post_path(post) %>
		</div>
		<% if current_user %>
			<div class="center">
				<%= button_to "delete", '/posts/destroy', method: :delete, params: {event: post } %>
			</div>
		<% end %>
	</section>
